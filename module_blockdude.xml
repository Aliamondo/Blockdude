    <mkdir dir="${blockdude.output.dir}"/>
    <javac2 destdir="${blockdude.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.blockdude}/javac">
      <compilerarg line="${compiler.args.blockdude}"/>
      <bootclasspath refid="blockdude.module.bootclasspath"/>
      <classpath refid="blockdude.module.production.classpath"/>
      <src refid="blockdude.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.blockdude"/>
    </javac2>
    
    <copy todir="${blockdude.output.dir}">
      <fileset dir="${module.blockdude.basedir}/Blockdude-android/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.blockdude.basedir}/Blockdude-desktop/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.blockdude.basedir}/Blockdude">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
